const input=document.getElementById('input')
    newsapi.v2.everything({
        sources: 'bbc-news,bbc-sport,abc-news',
        q:input.value,
        language: 'en'
    }).then(response => {
        var kk=response.articles;
        var headline="";
        var len=kk.length
        console.log(len)
        for(var i=1;i<len;i++)  
        {
            headline+=`<li>${response.articles[i].title}</li>`
            console.log(response.articles[i].title)
        }
        res.send(headline)
        }).catch(err=>{
            console.log(err);
        })